{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/Accessibilty/Accessibility.jsx", "components/Accessibilty/ColorAdjustment.jsx", "components/Accessibilty/Contactbox.jsx", "components/Accessibilty/Content_box1.jsx", "components/Accessibilty/Content_box2.jsx", "components/Accessibilty/Footer.jsx", "components/Accessibilty/Orientation.jsx", "app/page.tsx"],
  "exclude": ["node_modules"]
}
